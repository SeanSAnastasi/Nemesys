
@{
    ViewData["Title"] = "Reports";
}
<div class="container">
    <h1>Reports</h1>
    
    @foreach(var report in Model)
    {

<div class="container">
    <div class="container">
        <h4>@report.Title</h4>
        <p>@report.Description</p>
    </div>
    <div class="container">
        <button>Edit</button>
        <button>Delete</button>
        <button>@report.Likes</button>
        <a class="details-cta" asp-area="" asp-controller="Report" asp-action="Details" asp-route-id="@report.Id">View Details</a>
    </div>

</div>

    }
</div>


